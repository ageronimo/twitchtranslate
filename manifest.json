{
    "name": "Twitch Translate",
    "version": "0.0",
    "description": "Translate Twitch chat in real time.",
    "manifest_version": 3,
    "author": "Angela Geronimo",
    "action": {
        "default_popup": "./dist/index.html",
        "default_title": "Twitch Translate"
    },
    "host_permissions": [
        "https://translate.google.com/"
    ],
    "content_security_policy": {
        "script-src": "self unsafe-eval",
        "unsafe-eval": "object-src self",
        "unsafe-inline": "self"
    }
}